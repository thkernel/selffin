<% @expense_categories.each do |expense_category| %>
  <tr>
    
    <td><%= expense_category.name %></td>
    <td><%= expense_category.priority.name %></td>
    <td><%= expense_category.status %></td>
   <td>
                    <div class="action-buttons">
                    <%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'),  edit_expense_category_path(expense_category), title: "Modifier", remote: true %>




<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), expense_category_delete_path(expense_category), remote: true, title: "Supprimer", class: "delete" %>
</div>
</td>
  </tr>
<% end %>