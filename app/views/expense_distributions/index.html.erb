<p id="notice"><%= notice %></p>

<h1>Expense Distributions</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Expense category</th>
      <th>Percentage</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @expense_distributions.each do |expense_distribution| %>
      <tr>
        <td><%= expense_distribution.uid %></td>
        <td><%= expense_distribution.expense_category %></td>
        <td><%= expense_distribution.percentage %></td>
        <td><%= expense_distribution.status %></td>
        <td><%= expense_distribution.user %></td>
        <td><%= link_to 'Show', expense_distribution %></td>
        <td><%= link_to 'Edit', edit_expense_distribution_path(expense_distribution) %></td>
        <td><%= link_to 'Destroy', expense_distribution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Expense Distribution', new_expense_distribution_path %>
