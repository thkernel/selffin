<p id="notice"><%= notice %></p>

<h1>Income Expenses</h1>

<table>
  <thead>
    <tr>
      <th>Uid</th>
      <th>Income</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @income_expenses.each do |income_expense| %>
      <tr>
        <td><%= income_expense.uid %></td>
        <td><%= income_expense.income %></td>
        <td><%= income_expense.status %></td>
        <td><%= income_expense.user %></td>
        <td><%= link_to 'Show', income_expense %></td>
        <td><%= link_to 'Edit', edit_income_expense_path(income_expense) %></td>
        <td><%= link_to 'Destroy', income_expense, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Income Expense', new_income_expense_path %>
