<% @incomes.each do |income| %>
  <tr>
    <td><%= income.created_at.strftime("%d/%m/%Y") %></td>
    <td><%= income.uid %></td>
    <td><%= income.income_type.name %></td>
   
    <td><%= income.description %></td>
    <td><%= income.amount %></td>
    
    <td><%= income.status %></td>
    <td>
    <div class="action-buttons">
                    <%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'),  edit_income_path(income), title: "Modifier", remote: true %>


<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), income_delete_path(income), remote: true, title: "Supprimer", class: "delete" %>
</div>
</td>
  </tr>
<% end %>