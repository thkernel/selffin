<div class="br-pageheader">
      <nav class="breadcrumb pd-0 mg-0 tx-12">
        
        <span class="breadcrumb-item active"><h5>Sondages</h5></span>
      </nav>
    </div><!-- br-pageheader -->
<div class="br-pagebody mg-b-30">
  <div class="br-section-wrapper">
    <div class="headers mg-b-20">
        <div class="left-content">
        </div>

        <div class="text-right">
          <% if current_user.superuser? %>
            <%= link_to raw('<i class="fa fa-plus" aria-hidden="true"></i> Ajouter'), new_poll_path, class: "btn btn-primary tx-mont tx-medium tx-11 tx-uppercase pd-y-12 pd-x-25 tx-spacing-1", remote: true %>
            <% end %>

        </div>

    </div>


<table id="datatable1" class="table display responsive nowrap">

  <thead>
    <tr>
      <th>Crée le</th>
      <th>Catégorie</th>
      <th>Titre</th>
      <th>Date début</th>
      <th>Date fin</th>
     
      <th>Statut</th>
      <% if current_user.superuser? %>
        <th>Actions</th>
      <% end %>
    </tr>
  </thead>

  <tbody class="polls">
    <%= render "index" %>
  </tbody>
</table>
</div>
</div>
<div id="poll-modal" class="c-modal modal fade" data-backdrop="static"></div>
